{
    "name": "publicoms/laravel-custom-paginator",
    "description": "Paginate based on last item, not pages.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KARIM",
            "email": "ocpmstest@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "symfony/http-foundation": "~2.7|~3.0|~4.0|~5.0",
        "symfony/http-kernel": "~2.7|~3.0|~4.0|~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6|^7|^8|^9",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "publicoms\\ItemPaginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "publicoms\\ItemPaginator\\Tests\\" : "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "publicoms\\laravel-custom-paginator"
            ]
        }
    }
}
